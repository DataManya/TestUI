<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="First Priority" verbose="1">
    <listeners>
        <listener class-name="ru.seleniumtest.listener.ScreenshotListener"/>
    </listeners>
    <test name="Regression">
        <groups>
            <run>
                <include name="All"/>
            </run>
        </groups>
        <classes>
            <class name="ru.seleniumtest.tests.yandexmarket.CatalogTest"/>
            <class name="ru.seleniumtest.tests.yandexmarket.PriceTest"/>
        </classes>
    </test>
</suite>
